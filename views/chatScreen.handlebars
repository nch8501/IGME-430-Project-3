<html lang="en">
<head>
  <meta charset="utf-8"/>
  <title>Chat by Nick</title>
  <script crossorigin src="https://unpkg.com/react@15/dist/react.min.js"></script>
  <script crossorigin src="https://unpkg.com/react-dom@15/dist/react-dom.min.js"></script>
  <script crossorigin src="https://unpkg.com/prop-types/prop-types.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/babel-core/5.8.34/browser.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.2.0/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/remarkable/1.6.2/remarkable.min.js"></script>
  <link rel="shortcut icon" href="/assets/img/envelope.png"/>
  <link rel="stylesheet" type="text/css" href="/assets/style.css"/>
</head>
  
<body>
  
  <nav><a href="/login"><img id="logo" src="/assets/img/envelope.png" alt="logo"/></a>
    <div class="navLink"><a href="/chat">Chats</a></div>
    <div class="navLink"><a href="/account">Account</a></div>
    <div class="navlink"><a href="/logout">Log out</a></div> 
  </nav>
  <section id="chatHeader">
    <div>{{chat.title}}</div>
  </section>
  
  <section id="messageSection">
  </section>
  
  <section id="message">
  </section>
  
  <div id="domoMessage">
    <h3><span id="errorMessage"></span></h3>
  </div>
  
  <section id="chatId" style="display: none">{{chat._id}}</section>
  
  <script src="/assets/chatScreenBundle.js"></script>
</body>
</html>